.header_container{
  min-height: 100vh;
  overflow: hidden;
  position: relative;
  justify-content: flex-start;
  align-items: center;
  background-color: #AA081B;
}

.canvas{
  position: fixed;

  top: -200vh;
  left: -200vw;
}
.header{
  width: 300%;
  height: 300%;
  position: absolute;
  top: -100%;
  left: -100%;
  right: -100%;
  bottom: -100%;
  z-index: 1;
}
.over_hide{
  position: absolute;
  top: 430rpx;
  left: 15rpx;
  right: 15rpx;
  width: calc(100% - 30rpx);
  height: 410rpx;
  overflow: hidden;
  background-color: #fff;
  z-index: 1;
}
.image{
  width: 100%;
  height: 100%;
}
.frame{
  position: relative;
  width: 100%;
  pointer-events: none;
}
.frame::after{
  content: '';
  position: absolute;
  top: -50vh;
  left: -10vw;
  right: -10vw;
  bottom: -50vh;
  width: 100%;
  height: 100%;
  border-width: 50vh 10vw;
}
.frame image{
  width: 100%;
  display: block;
}

.header_input{
  background-color: #fff;
  text-align: center;
  color: #000;
}
.header_button,.header_button1,.header_button2{
  position: fixed;
  left: 0;
  right: 0;
  bottom: 50rpx;
  width: 220rpx;
  height: 100rpx;
  margin: auto;
  border-radius: 10rpx;
  text-align: center;
  z-index: 2;
}
.header_button1{
  right: 66.66666667%;
}
.header_button2{
  left: 66.66666667%;
}
.main_bg::after{
  border-radius: 10rpx;
  overflow: hidden;
}