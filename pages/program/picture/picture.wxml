<view class='header_container flex_column'>

  <view class='frame' bindtap='uploadImage'>
    <image src='{{image}}' mode='aspectFill' style='width: 80vw;height:80vw;'></image>
  </view>
  <input placeholder='请输入您的姓名' class='header_input header_button main_button' maxlength='33' bindinput='nameValue'></input>
  <view class='header_button main_button main_bg flex_row flex_center' hover-class='button_hover' bindtap='uploadImage'>上传照片</view>
  <!-- <navigator url='/pages/program/meetingHeaderMaterial/meetingHeaderMaterial' class='header_button main_button main_bg flex_row flex_center' hover-class='button_hover'>选择素材</navigator> -->
  <view class='header_button main_button main_bg flex_row flex_center' hover-class='button_hover' bindtap='submit'>确认提交</view>
</view>

