.address_list{
  background-color: #fff;
  padding: 30rpx;
  margin-bottom: 40rpx;
}
.address_header{
  align-items: center;
  font-size: 32rpx;
}
.address_body{
  margin: 40rpx 0 60rpx;
  font-size: 28rpx;
  color: #999;
}
.address_footer{
  font-size: 28rpx;
}
.radio{
  align-items: center;
}
.radio radio{
  zoom: 0.8;
  margin-right: 6rpx;
}

.address_button{
  margin: 0 30rpx 30rpx;
  border-radius: 15rpx;
}