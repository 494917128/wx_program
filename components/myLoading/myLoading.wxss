@import '/fonts/iconfont.wxss';
@import '/app.wxss';

.loading{
  font-size: 30rpx;
  height: 80rpx;
  color: #666;
}
.loading .iconfont{
  font-size: 24rpx;
  color: #999;
  margin: 10rpx;
  animation: loading linear 1.5s infinite;
}
@keyframes loading{
  from {transform: rotate(0deg)}
  to {transform: rotate(360deg)}
}